<p id="notice"><%= notice %></p>

<h1>Publicities</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Lien</th>
      <th>Situation</th>
      <th>Taille</th>
      <th>Statut</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @publicities.each do |publicity| %>
      <tr>
        <td><%= publicity.title %></td>
        <td><%= publicity.content %></td>
        <td><%= publicity.lien %></td>
        <td><%= publicity.situation %></td>
        <td><%= publicity.taille %></td>
        <td><%= publicity.statut %></td>
        <td><%= link_to 'Show', publicity %></td>
        <td><%= link_to 'Edit', edit_publicity_path(publicity) %></td>
        <td><%= link_to 'Destroy', publicity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publicity', new_publicity_path %>
